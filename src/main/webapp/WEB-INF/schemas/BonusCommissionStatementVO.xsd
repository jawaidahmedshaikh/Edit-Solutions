					</xs:simpleType>
				</xs:element>
				<xs:element name="PolicyNumber">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="20"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="BonusPremium" type="xs:decimal"/>
				<xs:element name="Type">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="20"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="AllocationPercent" type="xs:decimal"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="BonusStatementVO">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="StatementDate">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="10"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element ref="ParticipatingAgentInfoVO"/>
				<xs:element ref="BonusEarningsVO"/>
				<xs:element name="LastStatementAmount" type="xs:decimal"/>
				<xs:element name="LastStatementDate">
					<xs:simpleType>
						<xs:restriction base="xs:string"/>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="BonusEarningsVO">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="TaxableIncome" type="xs:decimal"/>
				<xs:element name="TaxableIncomeYTD" type="xs:decimal"/>
				<xs:element name="PositivePolicyEarnings" type="xs:decimal"/>
				<xs:element name="NegativePolicyEarnings" type="xs:decimal"/>
				<xs:element name="NetPolicyEarnings" type="xs:decimal"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="BonusCommissionStatementVO">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="BonusStatementVO" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="PremiumLevelStatementVO">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="IssueLevelPremium" type="xs:decimal"/>
				<xs:element name="ProductLevelIncreasePercent" type="xs:decimal"/>
				<xs:element name="ProductLevelIncreaseAmount" type="xs:decimal"/>
				<xs:element name="IncreaseStopAmount" type="xs:decimal"/>
				<xs:element ref="BonusCriteriaStatementVO" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="BonusCriteriaStatementVO">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="BonusAmount" type="xs:decimal"/>
				<xs:element name="BonusBasisPoint" type="xs:decimal"/>
				<xs:element name="ExcessBonusAmount" type="xs:decimal"/>
				<xs:element name="ExcessBonusBasisPoint" type="xs:decimal"/>
				<xs:element name="ExcessPremiumLevel" type="xs:decimal"/>
				<xs:element ref="BonusStatementLineVO" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
