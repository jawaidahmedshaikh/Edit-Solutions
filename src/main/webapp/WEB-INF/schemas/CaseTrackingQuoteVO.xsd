<xs:schema elementFormDefault="qualified" attributeFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:include schemaLocation="./EDITSolutions.xsd"/>
	<xs:element name="CaseTrackingQuoteVO">
		<xs:annotation>
			<xs:documentation>Quote processor</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="SegmentVO" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="ClientDetailVO" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="BeneAllocationPct" type="xs:decimal" minOccurs="0"/>
				<xs:element name="DisbursementDate" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="10"/>
							<xs:minLength value="10"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="DeathValue" type="xs:decimal" minOccurs="0"/>
				<xs:element name="Interest" type="xs:decimal" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
