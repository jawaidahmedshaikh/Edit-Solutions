<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="event.ChargeHistory" table="&quot;ChargeHistory&quot;"   >

        <id name="ChargeHistoryPK" column="&quot;ChargeHistoryPK&quot;" type="long" >
            <generator class="edit.services.db.hibernate.KeyGenerator"/>
        </id>

        <property name="ChargeAmount" column="&quot;ChargeAmount&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>
        <property name="ChargeTypeCT" column="&quot;ChargeTypeCT&quot;"/>
        <property name="AdjustmentTaxable" column="&quot;AdjustmentTaxable&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>
        <property name="AdjustmentNonTaxable" column="&quot;AdjustmentNonTaxable&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>

        <property name="EDITTrxHistoryFK" column="&quot;EDITTrxHistoryFK&quot;" insert="false" update="false"/>

        <many-to-one
            name="EDITTrxHistory"
            column="&quot;EDITTrxHistoryFK&quot;"
            class="event.EDITTrxHistory"
            cascade="none"
            />

    </class>
</hibernate-mapping>