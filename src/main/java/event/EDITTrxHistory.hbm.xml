        </set>

        <set name="InSuspenses" table="&quot;InSuspense&quot;" inverse="true"  cascade="all">
            <key column="&quot;EDITTrxHistoryFK&quot;"/>
            <one-to-many class="event.InSuspense"/>
        </set>

        <set name="ChargeHistories" table="&quot;ChargeHistory&quot;" inverse="true"  cascade="all">
            <key column="&quot;EDITTrxHistoryFK&quot;"/>
            <one-to-many class="event.ChargeHistory"/>
        </set>

        <set name="BucketHistories" table="&quot;BucketHistory&quot;" inverse="true"  cascade="all">
            <key column="&quot;EDITTrxHistoryFK&quot;"/>
            <one-to-many class="event.BucketHistory"/>
        </set>

        <set name="CommissionHistories" table="&quot;CommissionHistory&quot;" inverse="true"  cascade="all">
            <key column="&quot;EDITTrxHistoryFK&quot;"/>
            <one-to-many class="event.CommissionHistory"/>
        </set>

        <set name="InvestmentHistories" table="&quot;InvestmentHistory&quot;" inverse="true"  cascade="all">
            <key column="&quot;EDITTrxHistoryFK&quot;"/>
