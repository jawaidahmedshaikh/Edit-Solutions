        </set>

        <set name="CommissionablePremiumHistories" table="&quot;CommissionablePremiumHistory&quot;" inverse="true"  cascade="all">
            <key column="&quot;EDITTrxHistoryFK&quot;"/>
            <one-to-many class="event.CommissionablePremiumHistory"/>
        </set>

        <set name="ReinsuranceHistories" table="&quot;ReinsuranceHistory&quot;" inverse="true"  cascade="all">
            <key column="&quot;EDITTrxHistoryFK&quot;"/>
            <one-to-many class="reinsurance.ReinsuranceHistory"/>
        </set>


    </class>
</hibernate-mapping>