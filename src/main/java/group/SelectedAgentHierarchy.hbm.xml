<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    
    <class name="group.SelectedAgentHierarchy" table="&quot;SelectedAgentHierarchy&quot;"  >
        <id name="SelectedAgentHierarchyPK" column="&quot;SelectedAgentHierarchyPK&quot;" type="long">
            <generator class="edit.services.db.hibernate.KeyGenerator"/>
        </id>

        <property name="BatchContractSetupFK" column="&quot;BatchContractSetupFK&quot;" insert="false" update="false"/>
        <property name="AgentHierarchyFK" column="&quot;AgentHierarchyFK&quot;" insert="false" update="false"/>

        <many-to-one
            name="BatchContractSetup"
            column="&quot;BatchContractSetupFK&quot;"
            class="group.BatchContractSetup"
            cascade="none"/>

        <many-to-one
            name="AgentHierarchy"
            column="&quot;AgentHierarchyFK&quot;"
            class="contract.AgentHierarchy"
            cascade="none"/>

    </class>
</hibernate-mapping>
