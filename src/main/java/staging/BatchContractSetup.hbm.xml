<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    
    <class name="staging.BatchContractSetup" table="&quot;BATCHCONTRACTSETUP&quot;"  >
        <id name="BatchContractSetupPK" column="&quot;BATCHCONTRACTSETUPPK&quot;" type="long">
            <generator class="edit.services.db.hibernate.KeyGenerator"/>
        </id>

        <property name="GroupFK" column="&quot;GROUPFK&quot;" insert="false" update="false"/>
        <property name="EnrollmentFK" column="&quot;ENROLLMENTFK&quot;" insert="false" update="false"/>

        <property name="BatchID" column="&quot;BATCHID&quot;"/>
        <property name="Status" column="&quot;STATUS&quot;"/>
        <property name="NumberOfContracts" column="&quot;NUMBEROFCONTRACTS&quot;"/>
        <property name="AutoNumber" column="&quot;AUTONUMBER&quot;"/>
        <property name="ApplicationType" column="&quot;APPLICATIONTYPE&quot;"/>
        <property name="BatchEvent" column="&quot;BATCHEVENT&quot;"/>
        <property name="EffectiveDate" column="&quot;EFFECTIVEDATE&quot;" type="edit.services.db.hibernate.customtype.EDITDateUserType"/>
        <property name="ApplicationSignedDate" column="&quot;APPLICATIONSIGNEDDATE&quot;" type="edit.services.db.hibernate.customtype.EDITDateUserType"/>
        <property name="ApplicationSignedState" column="&quot;APPLICATIONSIGNEDSTATE&quot;"/>
        <property name="IssueState" column="&quot;ISSUESTATE&quot;"/>
        <property name="DeathBenefitOption" column="&quot;DEATHBENEFITOPTION&quot;"/>
        <property name="NonForfeitureOption" column="&quot;NONFORFEITUREOPTION&quot;"/>
        <property name="CreationOperator" column="&quot;CREATIONOPERATOR&quot;"/>
        <property name="CreationDate" column="&quot;CREATIONDATE&quot;" type="edit.services.db.hibernate.customtype.EDITDateUserType"/>
        <property name="ReceiptDate" column="&quot;RECEIPTDATE&quot;" type="edit.services.db.hibernate.customtype.EDITDateUserType"/>
        <property name="NumberOfWorkdays" column="&quot;NUMBEROFWORKDAYS&quot;"/>
        <property name="EnrollmentMethod" column="&quot;ENROLLMENTMETHOD&quot;"/>
        
        <property name="AdvancePercent" column="&quot;ADVANCEPERCENT&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>
        <property name="ApplicationReceivedDate" column="&quot;APPLICATIONRECEIVEDDATE&quot;" type="edit.services.db.hibernate.customtype.EDITDateUserType"/>
        <property name="RecoveryPercent" column="&quot;RECOVERYPERCENT&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>

        <set name="SegmentBases" table="&quot;SEGMENTBASE&quot;" inverse="true"   cascade="all">
            <key column="&quot;BATCHCONTRACTSETUPFK&quot;"/>
            <one-to-many class="staging.SegmentBase"/>
        </set>

        <many-to-one
            name="Group"
            column="&quot;GROUPFK&quot;"
            class="staging.Group"
            cascade="none"/>

        <many-to-one
            name="Enrollment"
            column="&quot;ENROLLMENTFK&quot;"
            class="staging.Enrollment"
            cascade="none"/>

    </class>
</hibernate-mapping>
