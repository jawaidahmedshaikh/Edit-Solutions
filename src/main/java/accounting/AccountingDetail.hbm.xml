        <property name="Source" column="&quot;Source&quot;"/>
        <property name="ContractClientFK" column="&quot;ContractClientFK&quot;" insert="false" update="false"/>

        <property name="PlacedAgentFK" column="&quot;PlacedAgentFK&quot;" insert="false" update="false"/>
        
        <many-to-one
            name="EDITTrx"
            column="&quot;EDITTrxFK&quot;"
            class="event.EDITTrx"
            cascade="none"
            />        

    </class>
</hibernate-mapping>

