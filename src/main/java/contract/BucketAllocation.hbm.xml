<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="contract.BucketAllocation" table="&quot;BucketAllocation&quot;"   >

        <id name="BucketAllocationPK" column="&quot;BucketAllocationPK&quot;" type="long" >
            <generator class="edit.services.db.hibernate.KeyGenerator"/>
        </id>

        <property name="AllocationPercent" column="&quot;AllocationPercent&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>
        <property name="Dollars" column="&quot;Dollars&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>
        <property name="Units" column="&quot;Units&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>

        <property name="BucketFK" column="&quot;BucketFK&quot;"  insert="false" update="false"/>

        <many-to-one
            name="Bucket"
            column="&quot;BucketFK&quot;"
            class="contract.Bucket"
            cascade="none"
            />

    </class>
</hibernate-mapping>