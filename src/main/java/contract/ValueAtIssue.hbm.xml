<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="contract.ValueAtIssue" table="&quot;ValueAtIssue&quot;"  >
        <id name="ValueAtIssuePK" column="&quot;ValueAtIssuePK&quot;" type="long">
            <generator class="edit.services.db.hibernate.KeyGenerator"/>
        </id>

        <property name="EffectiveDate" column="&quot;EffectiveDate&quot;" type="edit.services.db.hibernate.customtype.EDITDateUserType"/>
        <property name="GuaranteedPremium" column="&quot;GuaranteedPremium&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>
        <property name="IncreaseAmount" column="&quot;IncreaseAmount&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>

        <property name="SegmentFK" column="&quot;SegmentFK&quot;" insert="false" update="false"/>

        <many-to-one
            name="Segment"
            column="&quot;SegmentFK&quot;"
            class="contract.Segment"
            cascade="none"/>

    </class>
</hibernate-mapping>

