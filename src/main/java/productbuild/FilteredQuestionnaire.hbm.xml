<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="productbuild.FilteredQuestionnaire" table="&quot;FilteredQuestionnaire&quot;"  >
        <id name="FilteredQuestionnairePK" column="&quot;FilteredQuestionnairePK&quot;" type="long">
            <generator class="edit.services.db.hibernate.KeyGenerator"/>
        </id>

        <property name="QuestionnaireFK" column="&quot;QuestionnaireFK&quot;" insert="false" update="false"/>
        <property name="ProductStructureFK" column="&quot;ProductStructureFK&quot;"/>
        <property name="DisplayOrder" column="&quot;DisplayOrder&quot;"/>


        <set name="QuestionnaireResponses" table="&quot;QuestionnaireResponse&quot;" inverse="true" lazy="true" cascade="none">
            <key column="&quot;FilteredQuestionnaireFK&quot;"/>
            <one-to-many class="contract.QuestionnaireResponse"/>
        </set>        

        <many-to-one
            name="Questionnaire"
            column="&quot;QuestionnaireFK&quot;"
            class="productbuild.Questionnaire"
            cascade="none"/>

    </class>
</hibernate-mapping>

