<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="agent.AgentRequirement" table="&quot;AgentRequirement&quot;" >
        <id name="AgentRequirementPK" column="&quot;AgentRequirementPK&quot;" type="long">
            <generator class="edit.services.db.hibernate.KeyGenerator"/>
        </id>

        <property name="AgentFK" column="&quot;AgentFK&quot;" insert="false" update="false"/>
        <property name="FilteredRequirementFK" column="&quot;FilteredRequirementFK&quot;" insert="false" update="false"/>

        <property name="RequirementStatusCT" column="&quot;RequirementStatusCT&quot;"/>
        <property name="EffectiveDate" column="&quot;EffectiveDate&quot;" type="edit.services.db.hibernate.customtype.EDITDateUserType"/>
        <property name="FollowupDate" column="&quot;FollowupDate&quot;" type="edit.services.db.hibernate.customtype.EDITDateUserType"/>
        <property name="ReceivedDate" column="&quot;ReceivedDate&quot;" type="edit.services.db.hibernate.customtype.EDITDateUserType"/>

        <many-to-one
            name="Agent"
            column="&quot;AgentFK&quot;"
            class="agent.Agent"
            cascade="none"
            />

        <many-to-one
            name="FilteredRequirement"
            column="&quot;FilteredRequirementFK&quot;"
            class="contract.FilteredRequirement"
            cascade="none"/>
    </class>
</hibernate-mapping>
