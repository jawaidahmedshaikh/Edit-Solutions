<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="agent.ContributingProfile" table="&quot;ContributingProfile&quot;">

        <id name="ContributingProfilePK" column="&quot;ContributingProfilePK&quot;" type="long">
            <generator class="edit.services.db.hibernate.KeyGenerator"/>
        </id>

        <many-to-one
            name="BonusProgram"
            column="&quot;BonusProgramFK&quot;"
            class="agent.BonusProgram"
            cascade="none"
            />

        <many-to-one
            name="CommissionProfile"
            column="&quot;CommissionProfileFK&quot;"
            class="agent.CommissionProfile"
            cascade="none"
            />
    </class>
</hibernate-mapping>
