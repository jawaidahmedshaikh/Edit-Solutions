<?xml version="1.0"?><!DOCTYPE hibernate-mapping PUBLIC        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>    <class name="security.Operator" table="&quot;Operator&quot;" >        <id name="OperatorPK" column="&quot;OperatorPK&quot;" type="long">            <generator class="edit.services.db.hibernate.KeyGenerator"/>        </id>        <property name="Name" column="&quot;Name&quot;"/>        <property name="LastName" column="&quot;LastName&quot;"/>        <property name="FirstName" column="&quot;FirstName&quot;"/>        <property name="MiddleInitial" column="&quot;MiddleInitial&quot;"/>        <property name="Title" column="&quot;Title&quot;"/>        <property name="Dept" column="&quot;Dept&quot;"/>        <property name="MaintDateTime" column="&quot;MaintDateTime&quot;" type="edit.services.db.hibernate.customtype.EDITDateTimeUserType"/>        <property name="MaintOperator" column="&quot;MaintOperator&quot;"/>        <property name="EMail" column="&quot;EMail&quot;"/>        <property name="LockedIndicator" column="&quot;LockedIndicator&quot;"/>        <property name="LoggedInIndicator" column="&quot;LoggedInIndicator&quot;"/>        <property name="TerminationDate" column="&quot;TerminationDate&quot;" type="edit.services.db.hibernate.customtype.EDITDateUserType"/>        <property name="TelephoneNumber" column="&quot;TelephoneNumber&quot;"/>        <property name="TelephoneExtension" column="&quot;TelephoneExtension&quot;"/>                <property name="OperatorTypeCT" column="&quot;OperatorTypeCT&quot;"/>        <property name="ApplyMax" column="&quot;ApplyMax&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>        <property name="RemoveMax" column="&quot;RemoveMax&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>        <property name="TransferMax" column="&quot;TransferMax&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>        <property name="MaskFK" column="&quot;MaskFK&quot;" insert="false" update="false"/>       <set name="OperatorRoles" table="&quot;OperatorRole&quot;" inverse="true"  cascade="all" >            <key column="&quot;OperatorFK&quot;"/>            <one-to-many class="security.OperatorRole"/>        </set>                <set name="Passwords" table="&quot;Password&quot;" inverse="true"  cascade="all" >            <key column="&quot;OperatorFK&quot;"/>            <one-to-many class="security.Password"/>        </set>        <many-to-one            name="Mask"            column="&quot;MaskFK&quot;"            class="security.Mask"            cascade="none"/>    </class>    </hibernate-mapping>