        <property name="lntCode" column="LntCode"/>
        <property name="instypeShortLabel" column="InstypeShortLabel"/>
        <property name="policyEffectiveDate" column="PolicyEffectiveDate" type="date"/>
        <property name="FILLER" column="FILLER"/>
        <property name="test" type="java.lang.Boolean">
           <column name="isTest" sql-type="tinyint"/>
        </property>


    </class>
</hibernate-mapping>
