<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    
     <class name="com.editsolutions.prd.vo.AppSetting" table="PRD_AppSettings"  >
        <id name="appSettingPK" column="PRD_AppSettingsPK" type="long">
              <generator class="identity"/>
        </id>
        <property name="name" column="Name"/>
        <property name="value" column="Value"/>
        <property name="type" column="Type"/>
        <property name="description" column="Description"/>
        <property name="displayOrder" column="DisplayOrder"/>
        <property name="addDate" column="AddDate" type="date"/>
        <property name="addUser" column="AddUser"/>
        <property name="modDate" column="ModDate" type="date"/>
        <property name="modUser" column="ModUser"/>

    </class>
</hibernate-mapping>
