												<xs:restriction base="xs:string">
													<xs:pattern value="\p{Nd}{2}/\p{Nd}{2}/\p{Nd}{4}"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
                                        <xs:element name="DeductionAmountOverride" nillable="true">
                                            <xs:simpleType>
                                                <xs:restriction base="xs:decimal">
                                                    <xs:totalDigits value="19"/>
