<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="agent.BonusProgram" table="&quot;BonusProgram&quot;">

        <id name="BonusProgramPK" column="&quot;BonusProgramPK&quot;" type="long">
            <generator class="edit.services.db.hibernate.KeyGenerator"/>
        </id>

        <property name="ContractCodeCT" column="&quot;ContractCodeCT&quot;"/>
        <property name="CommissionLevelCT" column="&quot;CommissionLevelCT&quot;"/>
        <property name="BonusName" column="&quot;BonusName&quot;"/>
        <property name="ProduceCheckInd" column="&quot;ProduceCheckInd&quot;"/>
        <property name="FrequencyCT" column="&quot;FrequencyCT&quot;"/>
        <property name="BonusStartDate" column="&quot;BonusStartDate&quot;" type="edit.services.db.hibernate.customtype.EDITDateUserType"/>
        <property name="BonusStopDate" column="&quot;BonusStopDate&quot;" type="edit.services.db.hibernate.customtype.EDITDateUserType"/>
        <property name="ApplicationReceivedStopDate" column="&quot;ApplicationReceivedStopDate&quot;" type="edit.services.db.hibernate.customtype.EDITDateUserType"/>
        <property name="PremiumStopDate" column="&quot;PremiumStopDate&quot;" type="edit.services.db.hibernate.customtype.EDITDateUserType"/>
        <property name="IncludeAdditionalPremiumInd" column="&quot;IncludeAdditionalPremiumInd&quot;"/>
        <property name="SpecificParticipantStatus" column="&quot;SpecificParticipantStatus&quot;"/>
        <property name="SpecificProductStatus" column="&quot;SpecificProductStatus&quot;"/>
        <property name="SpecificCommissionProfStatus" column="&quot;SpecificCommissionProfStatus&quot;"/>
        <property name="SpecificHierarchyLevelStatus" column="&quot;SpecificHierarchyLevelStatus&quot;"/>
        <property name="ApplicationReceivedStartDate" column="&quot;ApplicationReceivedStartDate&quot;" type="edit.services.db.hibernate.customtype.EDITDateUserType"/>
        <property name="NextCheckDate" column="&quot;NextCheckDate&quot;" type="edit.services.db.hibernate.customtype.EDITDateUserType"/>
        <property name="BaseProgramCompleteInd" column="&quot;BaseProgramCompleteInd&quot;"/>

        <set name="ContributingProfiles" table="&quot;ContributingProfile&quot;" inverse="true"  cascade="all">
            <key column="&quot;BonusProgramFK&quot;"/>
            <one-to-many class="agent.ContributingProfile"/>
        </set>

        <set name="PremiumLevels" table="&quot;PremiumLevel&quot;" inverse="true"  cascade="all">
            <key column="&quot;BonusProgramFK&quot;"/>
            <one-to-many class="agent.PremiumLevel"/>
        </set>

        <set name="ParticipatingAgents" table="&quot;ParticipatingAgent&quot;" inverse="true"  cascade="all">
            <key column="&quot;BonusProgramFK&quot;"/>
            <one-to-many class="agent.ParticipatingAgent"/>
        </set>

    </class>
</hibernate-mapping>
