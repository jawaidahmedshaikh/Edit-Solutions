<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="agent.AgentNote" table="&quot;AgentNote&quot;" >
        <id name="AgentNotePK" column="&quot;AgentNotePK&quot;" type="long">
            <generator class="edit.services.db.hibernate.KeyGenerator"/>
        </id>

        <property name="AgentFK" column="&quot;AgentFK&quot;" insert="false" update="false"/>

        <property name="Sequence" column="&quot;Sequence&quot;"/>
        <property name="Note" column="&quot;Note&quot;"/>
        <property name="NoteTypeCT" column="&quot;NoteTypeCT&quot;"/>
        <property name="NoteQualifierCT" column="&quot;NoteQualifierCT&quot;"/>
        <property name="Operator" column="&quot;Operator&quot;"/>
        <property name="MaintDateTime" column="&quot;MaintDateTime&quot;" type="edit.services.db.hibernate.customtype.EDITDateTimeUserType"/>


        <many-to-one
            name="Agent"
            column="&quot;AgentFK&quot;"
            class="agent.Agent"
            cascade="none"
          />

    </class>
</hibernate-mapping>