<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
    "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="connection.datasource">java:comp/env/jdbc/staging</property>
        
        <property name="hibernate.jdbc.batch_size">50</property>
        <property name="hibernate.max_fetch_depth">6</property>
        <property name="hibernate.default_schema">dbo</property>
        <property name="hibernate.connection.driver_class">net.sourceforge.jtds.jdbc.Driver</property>
        <property name="hibernate.dialect">org.hibernate.dialect.SQLServerDialect</property>
        <property name="show_sql">false</property>
        
        <mapping resource="staging/Accounting.hbm.xml"></mapping>
        <mapping resource="staging/AdjustmentHistory.hbm.xml"></mapping>
        <mapping resource="staging/Agent.hbm.xml"></mapping>
        <mapping resource="staging/AnnualStatement.hbm.xml"></mapping>
        <mapping resource="staging/AnnualStatementMonthlySummary.hbm.xml"></mapping>
        <mapping resource="staging/BatchContractSetup.hbm.xml"></mapping>
        <mapping resource="staging/Bill.hbm.xml"></mapping>
        <mapping resource="staging/BillGroup.hbm.xml"></mapping>
        <mapping resource="staging/BillSchedule.hbm.xml"></mapping>
        <mapping resource="staging/Bucket.hbm.xml"></mapping>
        <mapping resource="staging/CalculatedValues.hbm.xml"></mapping>
        <mapping resource="staging/Case.hbm.xml"></mapping>
        <mapping resource="staging/CaseStatusHistory.hbm.xml"></mapping>
        <mapping resource="staging/CommissionActivity.hbm.xml"></mapping>
        <mapping resource="staging/CommissionPhase.hbm.xml"></mapping>
        <mapping resource="staging/ContractClient.hbm.xml"></mapping>
        <mapping resource="staging/ContractStatusHistory.hbm.xml"></mapping>
        <mapping resource="staging/ControlFundInfo.hbm.xml"></mapping>
        <mapping resource="staging/Enrollment.hbm.xml"></mapping>
        <mapping resource="staging/EnrollmentLeadServiceAgent.hbm.xml"></mapping>
        <mapping resource="staging/ErrorLog.hbm.xml"></mapping>
        <mapping resource="staging/FilteredProduct.hbm.xml"></mapping>
        <mapping resource="staging/FinancialActivity.hbm.xml"></mapping>
        <mapping resource="staging/GIOOptionValue.hbm.xml"></mapping>
        <mapping resource="staging/Group.hbm.xml"></mapping>
        <mapping resource="staging/Investment.hbm.xml"></mapping>
        <mapping resource="staging/InvestmentBucket.hbm.xml"></mapping>
        <mapping resource="staging/Payout.hbm.xml"></mapping>
        <mapping resource="staging/PayrollDeduction.hbm.xml"></mapping>
        <mapping resource="staging/PayrollDeductionSchedule.hbm.xml"></mapping>
        <mapping resource="staging/PremiumDue.hbm.xml"></mapping>
        <mapping resource="staging/ProjectedBusinessByMonth.hbm.xml"></mapping>
        <mapping resource="staging/ProjectionArray.hbm.xml"></mapping>
        <mapping resource="staging/Requirement.hbm.xml"></mapping>
        <mapping resource="staging/SegmentBase.hbm.xml"></mapping>
        <mapping resource="staging/SegmentRider.hbm.xml"></mapping>
        <mapping resource="staging/Staging.hbm.xml"></mapping>
        <mapping resource="staging/SubBucket.hbm.xml"></mapping>
        <mapping resource="staging/ValueAtIssue.hbm.xml"></mapping>
        <mapping resource="staging/CaseProductUnderwriting.hbm.xml"></mapping>
        <mapping resource="staging/EnrollmentState.hbm.xml"></mapping>
    </session-factory>
</hibernate-configuration>