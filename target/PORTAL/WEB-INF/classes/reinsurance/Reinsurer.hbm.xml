<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="reinsurance.Reinsurer" table="&quot;Reinsurer&quot;"  >
        <id name="ReinsurerPK" column="&quot;ReinsurerPK&quot;" type="long">
            <generator class="edit.services.db.hibernate.KeyGenerator"/>
        </id>

        <property name="ReinsurerNumber" column="&quot;ReinsurerNumber&quot;"/>

        <property name="ClientDetailFK" column="&quot;ClientDetailFK&quot;" insert="false" update="false"/>

        <set name="treaties" table="&quot;Treaty&quot;" inverse="true"  cascade="all">
            <key column="&quot;ReinsurerFK&quot;"/>
            <one-to-many class="reinsurance.Treaty"/>
        </set>

    </class>
</hibernate-mapping>

