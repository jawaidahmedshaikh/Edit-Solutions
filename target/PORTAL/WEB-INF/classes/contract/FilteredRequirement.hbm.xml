<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="contract.FilteredRequirement" table="&quot;FilteredRequirement&quot;"  >
        <id name="FilteredRequirementPK" column="&quot;FilteredRequirementPK&quot;" type="long">
            <generator class="edit.services.db.hibernate.KeyGenerator"/>
        </id>

        <property name="RequirementFK" column="&quot;RequirementFK&quot;" insert="false" update="false"/>
        <property name="ProductStructureFK" column="&quot;ProductStructureFK&quot;"/>

        <set name="CaseRequirements" table="&quot;CaseRequirement&quot;" inverse="true"  cascade="none">
            <key column="&quot;FilteredRequirementFK&quot;"/>
            <one-to-many class="casetracking.CaseRequirement"/>
        </set>

        <set name="AgentRequirements" table="&quot;AgentRequirement&quot;" inverse="true" lazy="true" cascade="none">
            <key column="&quot;FilteredRequirementFK&quot;"/>
            <one-to-many class="agent.AgentRequirement"/>
        </set>

        <set name="ContractGroupRequirements" table="&quot;ContractGroupRequirement&quot;" inverse="true" lazy="true" cascade="none">
            <key column="&quot;FilteredRequirementFK&quot;"/>
            <one-to-many class="group.ContractGroupRequirement"/>
        </set>  
        
        <many-to-one
            name="Requirement"
            column="&quot;RequirementFK&quot;"
            class="contract.Requirement"
            cascade="none"/>

    </class>
</hibernate-mapping>

