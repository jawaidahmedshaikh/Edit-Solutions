<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="contract.FilteredProduct" table="&quot;FilteredProduct&quot;"  >
        <id name="FilteredProductPK" column="&quot;FilteredProductPK&quot;" type="long">
            <generator class="edit.services.db.hibernate.KeyGenerator"/>
        </id>
        
        <property name="ContractGroupFK" column="&quot;ContractGroupFK&quot;" insert="false" update="false"/>
        <property name="ProductStructureFK" column="&quot;ProductStructureFK&quot;"/>
        
        <property name="EffectiveDate" column="&quot;EffectiveDate&quot;" type="edit.services.db.hibernate.customtype.EDITDateUserType"/>
        <property name="TerminationDate" column="&quot;TerminationDate&quot;" type="edit.services.db.hibernate.customtype.EDITDateUserType"/>
        <property name="MasterContractNumber" column="&quot;MasterContractNumber&quot;"/>
        <property name="Operator" column="&quot;Operator&quot;"/>
        <property name="CreationDate" column="&quot;CreationDate&quot;" type="edit.services.db.hibernate.customtype.EDITDateUserType"/>
        <property name="MasterContractName" column="&quot;MasterContractName&quot;"/>

        <many-to-one
            name="ContractGroup"
            column="&quot;ContractGroupFK&quot;"
            class="group.ContractGroup"
            cascade="none"/>

        <set name="MasterContracts" table="&quot;MasterContract&quot;" inverse="true"   cascade="all">
            <key column="&quot;FilteredProductFK&quot;"/>
            <one-to-many class="contract.MasterContract"/>
        </set>

    </class>
</hibernate-mapping>