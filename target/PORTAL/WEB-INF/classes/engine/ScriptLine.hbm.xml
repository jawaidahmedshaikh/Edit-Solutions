<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="engine.ScriptLine" table="&quot;ScriptLine&quot;">
    
        <cache usage="read-write"/>
    
        <id name="ScriptLinePK" column="&quot;ScriptLinePK&quot;" type="long">
            <generator class="edit.services.db.hibernate.KeyGenerator"/>
        </id>

        <property name="LineNumber" column="&quot;LineNumber&quot;"/>
        <property name="ScriptLine" column="&quot;ScriptLine&quot;"/>
        <property name="ScriptFK" column="&quot;ScriptFK&quot;" insert="false" update="false"/>

        <many-to-one
             name="Script"
             column="&quot;ScriptFK&quot;"
             class="engine.Script"
             cascade="none"/>

    </class>
</hibernate-mapping>

