<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="engine.Script" table="&quot;Script&quot;">
    
        <cache usage="read-write"/>
    
        <id name="ScriptPK" column="&quot;ScriptPK&quot;" type="long">
            <generator class="edit.services.db.hibernate.KeyGenerator"/>
        </id>

        <property name="ScriptName" column="&quot;ScriptName&quot;"/>
        <property name="Operator" column="&quot;Operator&quot;"/>
        <property name="MaintDateTime" column="&quot;MaintDateTime&quot;" type="edit.services.db.hibernate.customtype.EDITDateTimeUserType"/>
        <property name="ScriptTypeCT" column="&quot;ScriptTypeCT&quot;"/>
        <property name="ScriptStatusCT" column="&quot;ScriptStatusCT&quot;"/>



        <set name="ScriptLines" table="&quot;ScriptLine&quot;" inverse="true" cascade="all">
            <key column="&quot;ScriptFK&quot;"/>
            <one-to-many class="engine.ScriptLine"/>
        </set>

    </class>
</hibernate-mapping>