            eternal="true"
            timeToIdleSeconds="900"
            timeToLiveSeconds="900"
            overflowToDisk="true"
            diskPersistent="true"
            diskExpiryThreadIntervalSeconds="600"
            memoryStoreEvictionPolicy="LFU"
            />

