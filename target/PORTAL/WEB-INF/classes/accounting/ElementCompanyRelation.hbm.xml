<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="accounting.ElementCompanyRelation" table="&quot;ElementCompanyRelation&quot;">
    
        <cache usage="read-write"/>
    
        <id name="ElementCompanyRelationPK" column="&quot;ElementCompanyRelationPK&quot;" type="long">
            <generator class="edit.services.db.hibernate.KeyGenerator"/>
        </id>
        
        <property name="ProductStructureFK" column="&quot;ProductStructureFK&quot;" insert="false" update="false"/>
        <property name="ElementFK" column="&quot;ElementFK&quot;" insert="false" update="false"/>
        
        <many-to-one
            name="Element"
            column="&quot;ElementFK&quot;"
            class="accounting.Element"
            cascade="none"
         />
         
    </class>
</hibernate-mapping>

