<?xml version="1.0"?><!DOCTYPE hibernate-mapping PUBLIC        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>    <class name="security.Role" table="&quot;Role&quot;" >        <id name="RolePK" column="&quot;RolePK&quot;" type="long">            <generator class="edit.services.db.hibernate.KeyGenerator"/>        </id>       <property name="Name" column="&quot;Name&quot;"/>              <set name="FilteredRoles" table="&quot;FilteredRole&quot;" inverse="true"  cascade="all" >            <key column="&quot;RoleFK&quot;"/>            <one-to-many class="security.FilteredRole"/>       </set>       <set name="OperatorRoles" table="&quot;OperatorRole&quot;" inverse="true"  cascade="all" >            <key column="&quot;RoleFK&quot;"/>            <one-to-many class="security.OperatorRole"/>       </set>               <set name="ParentRoles" table="&quot;ImpliedRole&quot;" inverse="true"  cascade="all" >            <key column="&quot;ImpliedRoleFK&quot;"/>            <one-to-many class="security.ImpliedRole"/>        </set>    </class></hibernate-mapping>