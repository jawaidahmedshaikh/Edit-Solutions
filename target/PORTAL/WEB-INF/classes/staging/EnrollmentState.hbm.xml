<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    
    <class name="staging.EnrollmentState" table="&quot;ENROLLMENTSTATE&quot;"  >
        <id name="EnrollmentStatePK" column="&quot;ENROLLMENTSTATEPK&quot;" type="long">
            <generator class="edit.services.db.hibernate.KeyGenerator"/>
        </id>

        <property name="State" column="&quot;STATE&quot;"/>
        <property name="EnrollmentFK" column="&quot;ENROLLMENTFK&quot;" insert="false" update="false"/>

        <many-to-one
            name="Enrollment"
            column="&quot;ENROLLMENTFK&quot;"
            class="staging.Enrollment"
            cascade="none"/>

    </class>
</hibernate-mapping>
