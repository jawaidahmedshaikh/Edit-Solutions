<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="event.InSuspense" table="&quot;InSuspense&quot;"   >

        <id name="InSuspensePK" column="&quot;InSuspensePK&quot;" type="long" >
            <generator class="edit.services.db.hibernate.KeyGenerator"/>
        </id>

        <property name="Amount" column="&quot;Amount&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>
        <property name="SuspenseFK" column="&quot;SuspenseFK&quot;" insert="false" update="false"/>

        <property name="EDITTrxHistoryFK" column="&quot;EDITTrxHistoryFK&quot;"  insert="false" update="false"/>

        <many-to-one
            name="EDITTrxHistory"
            column="&quot;EDITTrxHistoryFK&quot;"
            class="event.EDITTrxHistory"
            cascade="none"

            />

        <many-to-one
            name="Suspense"
            column="&quot;SuspenseFK&quot;"
            class="event.Suspense"
            cascade="none"

            />

    </class>
</hibernate-mapping>