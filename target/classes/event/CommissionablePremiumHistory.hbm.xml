<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="event.CommissionablePremiumHistory" table="&quot;CommissionablePremiumHistory&quot;" >
        <id name="CommissionablePremiumHistoryPK" column="&quot;CommissionablePremiumHistoryPK&quot;" type="long">
            <generator class="edit.services.db.hibernate.KeyGenerator"/>
        </id>

        <property name="Duration" column="&quot;Duration&quot;"/>
        <property name="CommissionablePremium" column="&quot;CommissionablePremium&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>

        <property name="EDITTrxHistoryFK" column="&quot;EDITTrxHistoryFK&quot;" insert="false" update="false" />        
        <property name="CommissionPhaseFK" column="&quot;CommissionPhaseFK&quot;" insert="false" update="false" />

        <many-to-one
            name="EDITTrxHistory"
            column="&quot;EDITTrxHistoryFK&quot;"
            class="event.EDITTrxHistory"
            cascade="none"
            />

         <many-to-one
            name="CommissionPhase"
            column="&quot;CommissionPhaseFK&quot;"
            class="contract.CommissionPhase"
            cascade="none"
            />

    </class>
</hibernate-mapping>
