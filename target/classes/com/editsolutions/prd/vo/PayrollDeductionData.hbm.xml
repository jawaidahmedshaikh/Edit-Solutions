<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    
    <class name="com.editsolutions.prd.vo.PayrollDeductionData" table="PRD_Data"  >
        <id name="dataPK" column="PRD_DataPK" type="long">
              <generator class="identity"/>
        </id>

        <property name="dataHeaderFK" column="PRD_DataHeaderFK" type="long"/>
        <property name="fileTemplateFieldFK" column="PRD_FileTemplateFieldPK" type="long"/>
        <property name="recordFK" column="PRD_RecordFK" type="long"/>
        <property name="fieldValue" column="FieldValue"/>
        <property name="addDate" column="AddDate" type="date"/>
        <property name="addUser" column="AddUser"/>
        <property name="modDate" column="ModDate" type="date"/>
        <property name="modUser" column="ModUser"/>
        
        <many-to-one name="fileTemplateField" lazy="false" class="com.editsolutions.prd.vo.FileTemplateField"
             column="PRD_FileTemplateFieldPK" unique="true" insert="false" update="false"/>

        
    </class>
</hibernate-mapping>
