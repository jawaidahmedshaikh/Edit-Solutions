<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    
     <class name="com.editsolutions.prd.vo.SetupContact" table="PRD_SetupContact"  >
        <id name="setupContactPK" column="PRD_SetupContactPK" type="long">
              <generator class="identity"/>
        </id>
        <property name="setupFK" column="PRD_SetupFK"/>
        <property name="name" column="ContactName"/>
        <property name="email" column="ContactEmail"/>
        <property name="recipientTypeCT" column="RecipientTypeCT"/>
        <property name="addDate" column="AddDate" type="date"/>
        <property name="addUser" column="AddUser"/>
        <property name="modDate" column="ModDate" type="date"/>
        <property name="modUser" column="ModUser"/>

 


    </class>
</hibernate-mapping>
