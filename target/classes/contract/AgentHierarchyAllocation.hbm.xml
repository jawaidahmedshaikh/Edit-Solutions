<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="contract.AgentHierarchyAllocation" table="&quot;AgentHierarchyAllocation&quot;"  >
        <id name="AgentHierarchyAllocationPK" column="&quot;AgentHierarchyAllocationPK&quot;" type="long">
