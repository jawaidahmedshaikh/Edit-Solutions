<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="client.TaxProfile" table="&quot;TaxProfile&quot;"  >
        <id name="TaxProfilePK" column="&quot;TaxProfilePK&quot;" type="long">
            <generator class="edit.services.db.hibernate.KeyGenerator"/>
        </id>

        <property name="TaxFilingStatusCT" column="&quot;TaxFilingStatusCT&quot;"/>
        <property name="Exemptions" column="&quot;Exemptions&quot;"/>
        <property name="TaxIndicatorCT" column="&quot;TaxIndicatorCT&quot;"/>

        <property name="FicaIndicator" column="&quot;FicaIndicator&quot;"/>
        <property name="OverrideStatus" column="&quot;OverrideStatus&quot;"/>

        <property name="TaxInformationFK" column="&quot;TaxInformationFK&quot;" insert="false" update="false"/>

        <many-to-one
            name="TaxInformation"
            column="&quot;TaxInformationFK&quot;"
            class="client.TaxInformation"
            cascade="none"/>

    </class>
</hibernate-mapping>

