        <property name="YTDGrpRecoveries" column="&quot;YTDGRPRECOVERIES&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>

        <property name="YTDIndAdvancesOnly" column="&quot;YTDINDADVANCESONLY&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>
        <property name="YTDIndChargeback" column="&quot;YTDINDCHARGEBACK&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>
