        <property name="StagingFK" column="&quot;STAGINGFK&quot;" insert="false" update="false"/>

        <property name="HireDate" column="&quot;HIREDATE&quot;" type="edit.services.db.hibernate.customtype.EDITDateUserType"/>
        <property name="TerminationDate" column="&quot;TERMINATIONDATE&quot;" type="edit.services.db.hibernate.customtype.EDITDateUserType"/>
        <property name="AgentStatus" column="&quot;AGENTSTATUS&quot;"/>

        <property name="AgentType" column="&quot;AGENTTYPE&quot;"/>
        <property name="Department" column="&quot;DEPARTMENT&quot;"/>
        <property name="Region" column="&quot;REGION&quot;"/>

        <property name="Branch" column="&quot;BRANCH&quot;"/>
        <property name="DisbursementAddressType" column="&quot;DISBURSEMENTADDRESSTYPE&quot;"/>
        <property name="CorrespondenceAddressType" column="&quot;CORRESPONDENCEADDRESSTYPE&quot;"/>

        <property name="AgentNumber" column="&quot;AGENTNUMBER&quot;"/>
        <property name="LastName" column="&quot;LASTNAME&quot;"/>
        <property name="FirstName" column="&quot;FIRSTNAME&quot;"/>

        <property name="MiddleName" column="&quot;MIDDLENAME&quot;"/>
        <property name="AddressLine1" column="&quot;ADDRESSLINE1&quot;"/>
        <property name="AddressLine2" column="&quot;ADDRESSLINE2&quot;"/>

        <property name="AddressLine3" column="&quot;ADDRESSLINE3&quot;"/>
        <property name="AddressLine4" column="&quot;ADDRESSLINE4&quot;"/>
        <property name="City" column="&quot;CITY&quot;"/>

        <property name="State" column="&quot;STATE&quot;"/>
        <property name="Zip" column="&quot;ZIP&quot;"/>
