        <set name="Accountings" table="&quot;ACCOUNTING&quot;" inverse="true"   cascade="all">
            <key column="&quot;AGENTFK&quot;"/>
            <one-to-many class="staging.Accounting"/>
        </set>

        <set name="CommissionActivities" table="&quot;COMMISSIONACTIVITY&quot;" inverse="true"  cascade="all" >
            <key column="&quot;AGENTFK&quot;"/>
            <one-to-many class="staging.CommissionActivity"/>
        </set>

        <many-to-one
            name="Staging"
            column="&quot;STAGINGFK&quot;"
            class="staging.Staging"
            cascade="none"/>
            
        <many-to-one
            name="SegmentBase"
            column="SegmentBaseFK"
            class="staging.SegmentBase"
            cascade="none"/>
    </class>
</hibernate-mapping>

