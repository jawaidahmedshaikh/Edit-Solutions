        <property name="GroupRnwlEarn" column="&quot;GROUPRNWLEARN&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>
        <property name="GrpAdvances" column="&quot;GRPADVANCES&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>

        <property name="IndFYEarn" column="&quot;INDFYEARN&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>
        <property name="IndRnwlEarn" column="&quot;INDRNWLEARN&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>
        <property name="IndAdvances" column="&quot;INDADVANCES&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>

        <property name="YTDGroupFYEarn" column="&quot;YTDGROUPFYEARN&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>
        <property name="YTDGroupRnwlEarn" column="&quot;YTDGROUPRNWLEARN&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>
        <property name="YTDGrpAdvances" column="&quot;YTDGRPADVANCES&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>

        <property name="YTDIndFYEarn" column="&quot;YTDINDFYEARN&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>
        <property name="YTDIndRnwlEarn" column="&quot;YTDINDRNWLEARN&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>
        <property name="YTDIndAdvances" column="&quot;YTDINDADVANCES&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>

        <property name="CommBalance" column="&quot;COMMBALANCE&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>
        <property name="LifetimeCommBalance" column="&quot;LIFETIMECOMMBALANCE&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>
        <property name="LifetimeAdvanceBalance" column="&quot;LIFETIMEADVANCEBALANCE&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>

        <property name="PhoneNumber" column="&quot;PHONENUMBER&quot;"/>
        
        <property name="Advances" column="&quot;ADVANCES&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>
        <property name="AgentBalance" column="&quot;AGENTBALANCE&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>
