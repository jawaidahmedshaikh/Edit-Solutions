<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="agent.Redirect" table="&quot;Redirect&quot;" >
        <id name="RedirectPK" column="&quot;RedirectPK&quot;" type="long">
            <generator class="edit.services.db.hibernate.KeyGenerator"/>
        </id>

        <property name="AgentFK" column="&quot;AgentFK&quot;" insert="false" update="false"/>
        <property name="ClientRoleFK" column="&quot;ClientRoleFK&quot;" insert="false" update="false"/>

        <property name="RedirectTypeCT" column="&quot;RedirectTypeCT&quot;"/>
        <property name="StartDate" column="&quot;StartDate&quot;" type="edit.services.db.hibernate.customtype.EDITDateUserType"/>
        <property name="StopDate" column="&quot;StopDate&quot;" type="edit.services.db.hibernate.customtype.EDITDateUserType"/>

          <many-to-one
            name="Agent"
            column="&quot;AgentFK&quot;"
            class="agent.Agent"
            cascade="none"
            />

         <many-to-one
            name="ClientRole"
            column="&quot;ClientRoleFK&quot;"
            class="role.ClientRole"
            cascade="none"
            />

    </class>
</hibernate-mapping>
