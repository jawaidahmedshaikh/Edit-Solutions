<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="engine.sp.PRASEDocument" table="&quot;PRASEDocument&quot;">
        <id name="PRASEDocumentPK" column="&quot;PRASEDocumentPK&quot;" type="long">
