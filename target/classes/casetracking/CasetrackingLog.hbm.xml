<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="casetracking.CasetrackingLog" table="&quot;CasetrackingLog&quot;" lazy="true">
        <id name="CasetrackingLogPK" column="&quot;CasetrackingLogPK&quot;" type="long">
            <generator class="edit.services.db.hibernate.KeyGenerator"/>
        </id>

        <property name="CaseTrackingProcess" column="&quot;CaseTrackingProcess&quot;"/>
        <property name="CaseTrackingEvent" column="&quot;CaseTrackingEvent&quot;"/>
        <property name="ContractNumber" column="&quot;ContractNumber&quot;"/>
        <property name="EffectiveDate" column="&quot;EffectiveDate&quot;" type="edit.services.db.hibernate.customtype.EDITDateUserType"/>
        <property name="ProcessDate" column="&quot;ProcessDate&quot;" type="edit.services.db.hibernate.customtype.EDITDateUserType"/>
        <property name="ClientName" column="&quot;ClientName&quot;"/>
        <property name="AllocationPercent" column="&quot;AllocationPercent&quot;" type="edit.services.db.hibernate.customtype.EDITBigDecimalUserType"/>
        <property name="NewContractNumber" column="&quot;NewContractNumber&quot;"/>
        <property name="Operator" column="&quot;Operator&quot;"/>

        <property name="ClientDetailFK" column="&quot;ClientDetailFK&quot;" insert="false" update="false"/>

        <many-to-one
            name="ClientDetail"
            column="&quot;ClientDetailFK&quot;"
            class="client.ClientDetail"
            cascade="none"/>



    </class>
</hibernate-mapping>

