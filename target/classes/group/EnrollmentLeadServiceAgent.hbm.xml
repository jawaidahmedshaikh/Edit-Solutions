<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    
    <class name="group.EnrollmentLeadServiceAgent" table="&quot;EnrollmentLeadServiceAgent&quot;"  >
        <id name="EnrollmentLeadServiceAgentPK" column="&quot;EnrollmentLeadServiceAgentPK&quot;" type="long">
            <generator class="edit.services.db.hibernate.KeyGenerator"/>
        </id>

        <property name="EffectiveDate" column="&quot;EffectiveDate&quot;" type="edit.services.db.hibernate.customtype.EDITDateUserType"/>
        <property name="TerminationDate" column="&quot;TerminationDate&quot;" type="edit.services.db.hibernate.customtype.EDITDateUserType"/>
        <property name="RegionCT" column="&quot;RegionCT&quot;"/>

        <property name="EnrollmentFK" column="&quot;EnrollmentFK&quot;" insert="false" update="false"/>
        <property name="ClientRoleFK" column="&quot;ClientRoleFK&quot;" insert="false" update="false"/>

        <many-to-one
            name="Enrollment"
            column="&quot;EnrollmentFK&quot;"
            class="group.Enrollment"
            cascade="none"/>

        <many-to-one
            name="ClientRole"
            column="&quot;ClientRoleFK&quot;"
            class="role.ClientRole"
            cascade="none"/>

    </class>
</hibernate-mapping>
